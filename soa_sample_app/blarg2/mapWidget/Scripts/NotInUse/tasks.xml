<?xml version="1.0" encoding="utf-8"?>
<Configuration>
	<Tasks>
		<!-- POLICE Tasks -->
		<Task>
			<Type></Type>
			<Division></Division>
			<Owner></Owner>
			<TaskPoints>
				<!-- Repeat as many point tags as necessary -->
				<Point>
					<PointX></PointX>
					<PointY></PointY>
				</Point>
				<Point>
					<PointX></PointX>
					<PointY></PointY>
				</Point>
			</TaskPoints>
			<!-- Repeat as many details tags as necessary -->
			<Details></Details>
			<!-- Anything other than "Polygon" will result in the isClosed value of the task to be false -->
			<TaskShape></TaskShape>
			<!-- Milliseconds for timer to create this task -->
			<Time></Time>
		</Task>
	</Tasks>
</Configuration>
			
			
			
			
			
		